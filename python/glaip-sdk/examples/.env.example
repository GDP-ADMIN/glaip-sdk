# AI Agent Platform Examples Environment Configuration

# Copy this file to .env and fill in your actual values

# Required: Your API key for authentication
AIP_API_KEY=your-actual-api-key-here

# Required: Your AIP instance URL
AIP_API_URL=http://localhost:8000

# Optional: Timeout settings (in seconds)
AIP_TIMEOUT=300

# Optional: Debug mode (true/false)
AIP_DEBUG=false

# Optional: Log level (DEBUG, INFO, WARNING, ERROR)
AIP_LOG_LEVEL=INFO

# Optional: Custom model configuration
AIP_MODEL=gpt-4
AIP_MODEL_PROVIDER=openai
