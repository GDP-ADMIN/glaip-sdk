# AI Agent Platform SDK Examples Environment Configuration
# Copy this file to .env and fill in your actual values

# API Configuration (Required)
AIP_API_URL=http://localhost:8000
AIP_API_KEY=your-api-key-here

# Optional: Request timeout (seconds)
AIP_TIMEOUT=30.0

# Optional: Default model
AIP_DEFAULT_MODEL=gpt-4.1

# Optional: Backend health check URL
AIP_HEALTH_CHECK_URL=http://localhost:8000/health-check

# Optional: Logging level
AIP_LOG_LEVEL=INFO

# Optional: Organization and Project IDs
AIP_ORG_ID=default
AIP_PROJECT_ID=default

# Optional: Retry configuration
AIP_RETRY_MAX=3

# Optional: TLS verification
AIP_TLS_VERIFY=true
AIP_CA_BUNDLE=

# Optional: Concurrency settings
AIP_CONCURRENCY=4
